{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
   
        "airbnb",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes":["error","single"],
        "curly":"error",
        "eqeqeq":"off",
        "react/react-in-jsx-scope" :0,
        "no-console":0
    }
}
